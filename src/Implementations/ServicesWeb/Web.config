    </pages>
    <authentication mode="Forms">
      <forms name=".ADAuthCookie" loginUrl="~/account" timeout="45" slidingExpiration="true" protection="All" />
    </authentication>
    <membership defaultProvider="WindowsMembershipProvider">
      <providers>
        <clear />
        <add name="WindowsMembershipProvider" type="System.Web.Security.ActiveDirectoryMembershipProvider" connectionStringName="ActiveDirectoryConnection" attributeMapUsername="sAMAccountName" />
      </providers>
    </membership>
    <roleManager enabled="true" defaultProvider="CustomRoleProvider" domain="NATIVECODE">
      <providers>
        <clear />
        <add name="CustomRoleProvider" type="NativeCode.Web.Membership.WindowsMembershipRoleProvider" />
      </providers>
    </roleManager>
