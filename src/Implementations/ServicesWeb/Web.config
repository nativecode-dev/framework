    <add name="Default" providerName="System.Data.SqlClient" connectionString="Server=(local); Database=NativeCodeServices; Integrated Security=True; MultipleActiveResultSets=True;" />
  </connectionStrings>
  <system.web>
    <compilation debug="true" targetFramework="4.5.2" />
